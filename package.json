{
	"name": "express-starter",
	"version": "1.0.0",
	"description": "",
	"main": "server/index.js",
	"scripts": {
		"start": "env-cmd -f ./env/.env node server/index.js",
		"dev": "env-cmd -f ./env/dev.env nodemon server/index.js --config .nodemonrc.json | pino-pretty -c",
		"dev:debug": "env-cmd -f ./env/dev.env nodemon server/index.js --config .nodemonrc.json --inspect | pino-pretty -c ",
		"p": " prettier --write {server,test}/**/*.js ",
		"p:w": "onchange 'server/**/*.js' -- prettier --write {{changed}}",
		"lint": "eslint -c .eslintrc.json {server,test}/**",
		"lint:fix": "eslint --fix -c .eslintrc.json {server,test}/**",
		"test": "env-cmd -f ./env/test.env jest --config=jest.config.js --watch | pino-pretty -c"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.1.6",
		"express-mongo-sanitize": "^1.3.2",
		"express-pino-logger": "^4.0.0",
		"express-rate-limit": "^5.0.0",
		"fs-extra": "^8.1.0",
		"helmet": "^3.21.2",
		"hpp": "^0.2.2",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.27",
		"mongoose": "^5.7.1",
		"mongoose-unique-validator": "^2.0.3",
		"morgan": "^1.9.1",
		"ms": "^2.1.2",
		"multer": "^1.4.2",
		"nodemailer": "^6.3.1",
		"pino": "^5.13.6",
		"pluralize": "^8.0.0",
		"slugify": "^1.3.6",
		"validator": "^12.0.0",
		"xss-clean": "^0.1.1"
	},
	"devDependencies": {
		"concurrently": "^5.0.0",
		"cross-env": "^6.0.3",
		"env-cmd": "^10.0.1",
		"eslint": "6.1.0",
		"eslint-config-prettier": "^6.7.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"express-validator": "^6.3.0",
		"faker": "^4.1.0",
		"husky": "^3.1.0",
		"jest": "^24.9.0",
		"nodemon": "^1.19.4",
		"onchange": "^6.1.0",
		"pino-pretty": "^3.4.0",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"supertest": "^4.0.2",
		"yargs": "^15.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn p",
			"pre-push": "pretty-quick --staged"
		}
	}
}