    "env-cmd": "^10.1.0",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
