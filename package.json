{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "env-cmd -f .env node --stack-trace server/index.js",
    "dev": " nodemon --stack-trace server/index.js --config .nodemonrc.json --inspect ",
    "dev:debug": " nodemon server/index.js --config .nodemonrc.json --inspect ",
    "p": " prettier --write {server,test}/**/*.js ",
    "l": "eslint -c .eslintrc.json {server,test}/**",
    "l:f": "eslint --fix -c .eslintrc.json {server,test}/**",
    "test": "jest --config=jest.config.js --watch | pino-pretty -c",
    "pre": "npm run  p && npm run l:f",
    "sl": "npx sequelize-cli --options-path=.sequelizerc.js",
    "prepare": "husky install"
  },
  "author": "seaside",
  "dependencies": {
