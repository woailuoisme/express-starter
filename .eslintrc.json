{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "off",
    "spaced-comment": "warn",
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 1
  }
}
